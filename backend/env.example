# Tesla REST API Microservice - Environment Variables
# Bu dosyayı .env olarak kopyalayın ve gerçek değerlerinizi girin

# Tesla Configuration
TESLA_EMAIL=your-email@example.com

# Tesla OAuth URLs
TESLA_AUTH_URL=https://auth.tesla.com/oauth2/v3/authorize
TESLA_CALLBACK_URL=https://auth.tesla.com/void/callback

# App Configuration
PORT=5001
HOST=0.0.0.0
DEBUG=True

# Security
SECRET_KEY=your-secret-key-here

# Database Configuration (Supabase PostgreSQL)
DB_USER=postgres.your_project_id
DB_PASSWORD=your-password
DB_HOST=aws-0-eu-north-1.pooler.supabase.com
DB_PORT=6543
DB_NAME=postgres
DATABASE_URL=postgresql://postgres:your-password@db.your_project_id.supabase.co:5432/postgres

# Timezone
TIMEZONE=Europe/Istanbul

# Optional: Redis for caching
# REDIS_URL=redis://localhost:6379/0 