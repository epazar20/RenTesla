# RenTesla Mobile Backend - Environment Variables
# Bu dosyayı .env olarak kopyalayın ve gerçek değerlerinizi girin

# Database Configuration (Supabase PostgreSQL)
DATABASE_URL=jdbc:postgresql://aws-0-eu-north-1.pooler.supabase.com:6543/postgres
DB_USER=postgres.your_project_id
DB_PASSWORD=your-password

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-here-should-be-at-least-256-bits

# Admin Configuration
ADMIN_PASSWORD=admin123

# Tesla API Integration
TESLA_API_BASE_URL=http://localhost:8000

# Spring Profiles
SPRING_PROFILES_ACTIVE=development 